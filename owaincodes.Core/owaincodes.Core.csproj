<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net472</TargetFramework>
  </PropertyGroup>
	
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="XCOPY /s /y &quot;$(TargetDir)$(ProjectName).*&quot; &quot;$(ProjectDir)\..\$(SolutionName).Web\bin\&quot;" />
  </Target>

	<ItemGroup>
		<PackageReference Include="Our.Umbraco.Extensions.Search" Version="1.4.1" />
		<PackageReference Include="UmbracoCms.Core" Version="8.8.0" />
		<PackageReference Include="UmbracoCms.Web" Version="8.8.0" />
	</ItemGroup>
</Project>